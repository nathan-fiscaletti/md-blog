<html>
    <head>
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="stylesheet" href="./assets/css/style.css?v=35">
        <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

        <link rel="stylesheet" href="./assets/css/prism.css" />
        <script src="{{FONT_AWESOME_KIT}}" crossorigin="anonymous"></script>
        <title>{{PAGE_TITLE}}</title>
    </head>
    <body>
        <div class="container content">
            <div class="post-nav-wrapper">
                <a href="index.html"><i class="fas fa-chevron-left"></i> Home</a>
            </div>
            <h1>
                {{TITLE}}
            </h1>
            <div class="post-preview-date">
                Last edited on {{DATE}}
            </div>
            <div class="post-preview-badges">
                {{BADGES}}
            </div>
            <div class="post-text">
{{POST}}
            </div>

            <hr style="width: 33%;margin-top: 64px;" />

            <div class="row">
                <div class="col-md-6">
                    <div class="media author-wrapper">
                        <img class="mr-3 rounded-circle author-avatar" alt="{{AUTHOR_NAME}}" src="{{AUTHOR_IMAGE}}" />
                        <div class="media-body">
                            <h5 class="author-name">
                                {{AUTHOR_NAME}}
                            </h5>
                            {{AUTHOR_BIO}}
                        </div>
                    </div>
                </div>
                <div class="col-md-6 author-wrapper">
                    <div class="a2a_kit a2a_default_style social-wrapper">
                        {{SOCIAL_SHARE_BUTTONS}}
                    </div>
                </div>
            </div>

            <div class="row more-posts-wrapper">
                <div class="col-md-6">
                    {{LAST_POST}}
                </div>
                <div class="col-md-6">
                    {{NEXT_POST}}
                </div>
            </div>

            <div class="copyright-wrapper">
                <span class="copyright">Copyright &copy; {{COPYRIGHT_YEAR}}</span>
            </div>
        </div>
        
        <!-- Javascript -->
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha256-pasqAKBDmFT4eHoN2ndd6lN370kFiGUFyTiUHWhU7k8=" crossorigin="anonymous"></script>
        <script src="assets/js/prism.js"></script>
        <script type="text/javascript">
            $("pre code").not("[class^='language-']").each(function(index) {
                Prism.highlightElement(this);
            });
        </script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js" integrity="sha384-6khuMg9gaYr5AxOqhkVIODVIvm9ynTT5J4V1cfthmT+emCG6yVmEZsRHdxlotUnm" crossorigin="anonymous"></script>
        <script async src="https://static.addtoany.com/menu/page.js"></script>
    </body>
</html>